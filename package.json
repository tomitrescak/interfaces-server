  "scripts": {
    "dev": "TS_NODE_TRANSPILE_ONLY=true nodemon -e ts,graphql -x node --inspect=5858 -r ts-node/register src/index.ts",
    "start": "forever start -a --watch --uid interfaces -c \"node --nolazy -r ts-node/register/transpile-only\" src/index.ts",
    "stop": "forever stop interfaces",
    "gql": "gql-gen --config codegen.yml",
    "db": "schemats generate -c mysql://root:prisma@localhost/confiscations -o ./src/generated/db.ts"
  },
